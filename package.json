{
  "name": "nodebb-theme-harmony",
  "version": "2.1.15",
  "nbbpm": {
    "compatibility": "^4.0.0"
  },
  "description": "Harmony theme for NodeBB",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeBB/nodebb-theme-harmony"
  },
  "scripts": {
    "lint": "eslint .",
      "build:css": "tailwindcss -i ./theme.scss -o ./public/style.css --minify",
      "dev:css": "tailwindcss -i ./theme.scss -o ./public/style.css --watch"
  },
  "keywords": [
    "nodebb",
    "theme",
    "forum",
    "bootstrap",
    "responsive"
  ],
  "contributors": [
    {
      "name": "Julian Lam",
